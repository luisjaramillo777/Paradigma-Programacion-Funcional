from functools import reduce
# Definición de una función pura para sumar una lista de números
def sumar_lista(lista):
return reduce(lambda x, y: x + y, lista, 0)
# Uso de la función
if __name__ == "__main__":
lista = [1, 2, 3, 4, 5]
resultado = sumar_lista(lista)
print(f"La suma de la lista es: {resultado}")  # Resultado: 15
